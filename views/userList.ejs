<html >
    
    <head>
        <meta charset="utf-8">
    </head>
    
    <body>
        <section class="home">
        <h1>Users list</h1>
        <ul class="list-group">
            <% for(var i=0; i<users.length; i++) {%>
                <li class="list-group-item">
                    <span>Name: </span><%= users[i].name %>
                    <br/>
                    <span>Occupation: </span><%= users[i].occupation %>
                    </br>
                    <span>Age: </span><%= users[i].age %>
                    <br/>
                    <span>Gender: </span><%= users[i].sex %>
                    <br/>
                </li>
            <% } %>
        </ul>
        </section>
    </body>
    
    </html>